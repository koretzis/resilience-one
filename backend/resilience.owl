<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://test.org/resilience.owl"
         xmlns="http://test.org/resilience.owl#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#">

<owl:Ontology rdf:about="http://test.org/resilience.owl"/>

<owl:ObjectProperty rdf:about="#supplies">
  <rdfs:domain rdf:resource="#PowerSubstation"/>
  <rdfs:range rdf:resource="#CriticalAsset"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_backup">
  <rdfs:domain rdf:resource="#CriticalAsset"/>
  <rdfs:range rdf:resource="#BackupGenerator"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasLoad">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#PowerSubstation"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasAmbientTemp">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#PowerSubstation"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasFuelLevel">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#BackupGenerator"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#InfrastructureNode">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#PowerSubstation">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#CriticalAsset">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#BackupGenerator">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#FailedNode">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#OverloadedNode">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#LowFuelGenerator">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#TotalBlackout">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#GridUnstable">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#sub-syntagma">
  <rdf:type rdf:resource="#PowerSubstation"/>
  <supplies rdf:resource="#hosp-evangelismos"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#sub-omonia">
  <rdf:type rdf:resource="#PowerSubstation"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#hosp-evangelismos">
  <rdf:type rdf:resource="#CriticalAsset"/>
  <has_backup rdf:resource="#gen-evangelismos"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#gen-evangelismos">
  <rdf:type rdf:resource="#BackupGenerator"/>
</owl:NamedIndividual>

<swrl:Variable rdf:about="urn:swrl#s"/>

<swrl:Variable rdf:about="urn:swrl#a"/>

<swrl:Variable rdf:about="urn:swrl#g"/>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#PowerSubstation"/>
          <swrl:argument1 rdf:resource="urn:swrl#s"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:ClassAtom>
              <swrl:classPredicate rdf:resource="#OverloadedNode"/>
              <swrl:argument1 rdf:resource="urn:swrl#s"/>
            </swrl:ClassAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:IndividualPropertyAtom>
                  <swrl:propertyPredicate rdf:resource="#supplies"/>
                  <swrl:argument1 rdf:resource="urn:swrl#s"/>
                  <swrl:argument2 rdf:resource="urn:swrl#a"/>
                </swrl:IndividualPropertyAtom>
              </rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#GridUnstable"/>
          <swrl:argument1 rdf:resource="urn:swrl#a"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#PowerSubstation"/>
          <swrl:argument1 rdf:resource="urn:swrl#s"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:IndividualPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#supplies"/>
              <swrl:argument1 rdf:resource="urn:swrl#s"/>
              <swrl:argument2 rdf:resource="urn:swrl#a"/>
            </swrl:IndividualPropertyAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:ClassAtom>
                  <swrl:classPredicate rdf:resource="#FailedNode"/>
                  <swrl:argument1 rdf:resource="urn:swrl#s"/>
                </swrl:ClassAtom>
              </rdf:first>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first>
                    <swrl:ClassAtom>
                      <swrl:classPredicate rdf:resource="#CriticalAsset"/>
                      <swrl:argument1 rdf:resource="urn:swrl#a"/>
                    </swrl:ClassAtom>
                  </rdf:first>
                  <rdf:rest>
                    <rdf:Description>
                      <rdf:first>
                        <swrl:IndividualPropertyAtom>
                          <swrl:propertyPredicate rdf:resource="#has_backup"/>
                          <swrl:argument1 rdf:resource="urn:swrl#a"/>
                          <swrl:argument2 rdf:resource="urn:swrl#g"/>
                        </swrl:IndividualPropertyAtom>
                      </rdf:first>
                      <rdf:rest>
                        <rdf:Description>
                          <rdf:first>
                            <swrl:ClassAtom>
                              <swrl:classPredicate rdf:resource="#LowFuelGenerator"/>
                              <swrl:argument1 rdf:resource="urn:swrl#g"/>
                            </swrl:ClassAtom>
                          </rdf:first>
                          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                        </rdf:Description>
                      </rdf:rest>
                    </rdf:Description>
                  </rdf:rest>
                </rdf:Description>
              </rdf:rest>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#TotalBlackout"/>
          <swrl:argument1 rdf:resource="urn:swrl#a"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>


</rdf:RDF>
