<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://test.org/resilience.owl"
         xmlns="http://test.org/resilience.owl#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#">

<owl:Ontology rdf:about="http://test.org/resilience.owl"/>

<owl:ObjectProperty rdf:about="#supplies">
  <rdfs:domain rdf:resource="#PowerSubstation"/>
  <rdfs:range rdf:resource="#CriticalAsset"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_backup">
  <rdfs:domain rdf:resource="#CriticalAsset"/>
  <rdfs:range rdf:resource="#BackupGenerator"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#is_redundant_to">
  <rdfs:domain rdf:resource="#PowerSubstation"/>
  <rdfs:range rdf:resource="#PowerSubstation"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#InfrastructureNode">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#PowerSubstation">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#CriticalAsset">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#BackupGenerator">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#FailedNode">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#OverloadedNode">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#LowFuelGenerator">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#GridUnstable">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<owl:Class rdf:about="#TotalBlackout">
  <rdfs:subClassOf rdf:resource="#InfrastructureNode"/>
</owl:Class>

<swrl:Variable rdf:about="urn:swrl#p1"/>

<swrl:Variable rdf:about="urn:swrl#a"/>

<swrl:Variable rdf:about="urn:swrl#p2"/>

<swrl:Variable rdf:about="urn:swrl#g"/>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#GridUnstable"/>
          <swrl:argument1 rdf:resource="urn:swrl#a"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:IndividualPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#has_backup"/>
              <swrl:argument1 rdf:resource="urn:swrl#a"/>
              <swrl:argument2 rdf:resource="urn:swrl#g"/>
            </swrl:IndividualPropertyAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:ClassAtom>
                  <swrl:classPredicate rdf:resource="#LowFuelGenerator"/>
                  <swrl:argument1 rdf:resource="urn:swrl#g"/>
                </swrl:ClassAtom>
              </rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#TotalBlackout"/>
          <swrl:argument1 rdf:resource="urn:swrl#a"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:IndividualPropertyAtom>
          <swrl:propertyPredicate rdf:resource="#supplies"/>
          <swrl:argument1 rdf:resource="urn:swrl#p1"/>
          <swrl:argument2 rdf:resource="urn:swrl#a"/>
        </swrl:IndividualPropertyAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:IndividualPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#is_redundant_to"/>
              <swrl:argument1 rdf:resource="urn:swrl#p2"/>
              <swrl:argument2 rdf:resource="urn:swrl#p1"/>
            </swrl:IndividualPropertyAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:ClassAtom>
                  <swrl:classPredicate rdf:resource="#FailedNode"/>
                  <swrl:argument1 rdf:resource="urn:swrl#p1"/>
                </swrl:ClassAtom>
              </rdf:first>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first>
                    <swrl:ClassAtom>
                      <swrl:classPredicate rdf:resource="#OverloadedNode"/>
                      <swrl:argument1 rdf:resource="urn:swrl#p2"/>
                    </swrl:ClassAtom>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </rdf:rest>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#GridUnstable"/>
          <swrl:argument1 rdf:resource="urn:swrl#a"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>


</rdf:RDF>
